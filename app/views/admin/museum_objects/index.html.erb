<p id="notice"><%= notice %></p>

<h1>Museum Objects</h1>

<table>
  <thead>
    <tr>
      <th>Systemnumber</th>
      <th>Objecttype</th>
      <th>Title</th>
      <th>Image</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @museum_objects.each do |museum_object| %>
      <tr>
        <td><%= museum_object.systemNumber %></td>
        <td><%= museum_object.objectType %></td>
        <td><%= museum_object.title %></td>
        <td><%= museum_object.image_id %></td>
        <td><%= museum_object.description %></td>
        <td><%= link_to 'Show', admin_museum_object_path(museum_object) %></td>
        <td><%= link_to 'Edit', edit_admin_museum_object_path(museum_object) %></td>
        <td><%= button_to 'Destroy', admin_museum_object_path(museum_object), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Museum Object', new_admin_museum_object_path %>
